<mapper namespace="com.znv.manage.dao.EmergencySuppliesDao">
    <select id="emergencySuppliesCoordinatesList" resultType="java.util.Map">
        select
            id,
            district,
            storage_pool_name storagePoolName,
            CONCAT(warehouse_space,"平方米") warehouseSpace,
            competent_department competentDepartment,
            address,
            longitude,
            latitude,
            remark
        from emergency_supplies
    </select>

