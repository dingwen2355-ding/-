<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.znv.manage.dao.SystemLinkDao">

    <select id="getSystemLink" resultType="java.util.HashMap">
        select
         *
         FROM t_system_link
        order by istop asc,sort asc
    </select>


</mapper>