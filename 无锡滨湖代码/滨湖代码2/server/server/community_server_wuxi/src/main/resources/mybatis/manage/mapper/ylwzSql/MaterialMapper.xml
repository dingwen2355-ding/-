<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.znv.manage.ylwz.dao.MaterialDao">

    <select id="queryMaterialList" resultType="map">
        SELECT
            *
        FROM
            tb_material_reserve where to_days(create_time) = to_days(now())
    </select>

    <select id="queryAllMaterialList" resultType="map">
        SELECT
            *
        FROM
            tb_material_reserve
    </select>

</mapper>