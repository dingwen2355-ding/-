<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.znv.manage.dao.CityFirmwareDao">

    <resultMap type="com.znv.manage.common.bean.CityFirmware" id="CityFirmwareMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="name" column="name" jdbcType="VARCHAR"/>
        <result property="parentType" column="parent_type" jdbcType="VARCHAR"/>
        <result property="type" column="type" jdbcType="VARCHAR"/>
        <result property="gpsx" column="gpsx" jdbcType="VARCHAR"/>
        <result property="gpsy" column="gpsy" jdbcType="VARCHAR"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="CityFirmwareMap">
        select
          id, `name`, parent_type, `type`, gpsx, gpsy, create_time
        from t_city_firmware
        where id = #{id}
    </select>

    <!--统计总行数-->
    <select id="count" resultType="java.lang.Long">
        select count(1)
        from t_city_firmware
        <where>
            <if test="id != null">
                and id = #{id}
            </if>
            <if test="name != null and name != ''">
                and `name` = #{name}
            </if>
            <if test="parentType != null and parentType != ''">
                and parent_type = #{parentType}
            </if>
            <if test="type != null and type != ''">
                and `type` = #{type}
            </if>
            <if test="gpsx != null and gpsx != ''">
                and gpsx = #{gpsx}
            </if>
            <if test="gpsy != null and gpsy != ''">
                and gpsy = #{gpsy}
            </if>
            <if test="createTime != null">
                and create_time = #{createTime}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into t_city_firmware(`name`, parent_type, `type`, gpsx, gpsy)
        values (#{name},#{parentType},#{type},#{gpsx},#{gpsy})
    </insert>

    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into t_city_firmware(`name`, parent_type, `type`, gpsx, gpsy)
        values
        <foreach collection="entities" item="entity" separator=",">
        (#{entity.name}, #{entity.parentType}, #{entity.type}, #{entity.gpsx}, #{entity.gpsy})
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into t_city_firmware(`name`,parent_type,`type`,gpsx,gpsy)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.name}, #{entity.parentType}, #{entity.type}, #{entity.gpsx}, #{entity.gpsy})
        </foreach>
        on duplicate key update
        `name` = values(`name`),
        parent_type = values(parent_type),
        `type` = values(`type`),
        gpsx = values(gpsx),
        gpsy = values(gpsy)
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update t_city_firmware
        <set>
            <if test="name != null and name != ''">
                name = #{name},
            </if>
            <if test="parentType != null and parentType != ''">
                parent_type = #{parentType},
            </if>
            <if test="type != null and type != ''">
                type = #{type},
            </if>
            <if test="gpsx != null and gpsx != ''">
                gpsx = #{gpsx},
            </if>
            <if test="gpsy != null and gpsy != ''">
                gpsy = #{gpsy},
            </if>
            <if test="createTime != null">
                create_time = #{createTime}
            </if>
        </set>
        where id = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from t_city_firmware where id = #{id}
    </delete>
    
    <!--通过主键批量删除-->
    <delete id="deleteBatch">
        delete from t_city_firmware where find_in_set(id,#{ids})
    </delete>

    <!--条件查询-->
    <select id="queryByCondition" resultMap="CityFirmwareMap">
        select
          id, `name`, parent_type, `type`, gpsx, gpsy, create_time
        from t_city_firmware
        <where>
            <if test="id != null">
                and id = #{id}
            </if>
            <if test="name != null and name != ''">
                and `name` = #{name}
            </if>
            <if test="parentType != null and parentType != ''">
                and parent_type = #{parentType}
            </if>
            <if test="type != null and type != ''">
                and `type` = #{type}
            </if>
        </where>
    </select>
</mapper>

