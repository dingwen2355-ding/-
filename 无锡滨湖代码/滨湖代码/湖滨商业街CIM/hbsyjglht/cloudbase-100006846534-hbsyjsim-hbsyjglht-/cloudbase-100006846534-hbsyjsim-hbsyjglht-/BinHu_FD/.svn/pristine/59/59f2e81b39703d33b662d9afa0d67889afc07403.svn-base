<template>
  <div class="main">
  <keep-alive>
    <router-view v-if="keepAlive"  />
  </keep-alive>
  <router-view v-if="!keepAlive" :key="$route.name" />
  </div>
</template>

<script>
  export default {
    name: "RouteView",
    computed: {
      keepAlive () {
        return this.$route.meta.keepAlive
      }
    },
  }
</script>