<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.wxgis.jc.system.mapper.SysDeptMapper">

    <select id="findResponseById" resultType="cn.wxgis.jc.system.resp.SysDeptResponse">
        select d.*, pd.name parentName from sys_dept d left join sys_dept pd on pd.id = d.parent_id
        where d.id = #{id}
    </select>

</mapper>
